{ "proxy_config": {

"content": {
      "id": "oidc",
      "backend_version": "oauth",
      "proxy": {
        "authentication_method": "oidc",
        "oidc_issuer_endpoint": "http://localhost:8000/openam/oauth2/realms/root",
        "api_backend": "http://127.0.0.1:8081",
        "hostname_rewrite": "echo",
        "hosts": [
          "localhost",
          "127.0.0.1"
        ],
        "backend": {
          "endpoint": "http://127.0.0.1:8081",
          "host": "echo"
        },
        "policy_chain": [
          { "name": "apicast.policy.oidc_default_configuiration", "configuration": {
            "overrides": [
              {"key": "public_key", "value": "" }
            ]
          } },
          { "name": "apicast.policy.apicast" }
        ],
        "proxy_rules": [
          {
            "http_method": "GET",
            "pattern": "/",
            "metric_system_name": "hits",
            "delta": 1
          },
          {
            "http_method": "POST",
            "pattern": "/",
            "metric_system_name": "hits",
            "delta": 1
          },
          {
            "http_method": "PUT",
            "pattern": "/",
            "metric_system_name": "hits",
            "delta": 1
          }
        ]
      }
}
}
}
