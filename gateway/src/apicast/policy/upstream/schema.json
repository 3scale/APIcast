{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Upstream policy configuration",
  "type": "object",
  "properties": {
    "rules": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "regex": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "required": ["regex", "url"]
      }
    }
  }
}
